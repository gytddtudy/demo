                                pos_hint: {'center_x': .5, 'center_y': 0}

                                MDBoxLayout:
                                    orientation: 'vertical'

                                    MDIconButton:
                                        id: input_card_head
                                        icon: 'dots-horizontal'
                                        pos_hint: {'center_x': .95}
                                        on_release: pass
                                    
                                    

                                    TextInput:
                                        id: user_input_text_f
                                        hint_text: "..."
                                        text: ''
                                        background_color: 0, 0, 0, 0
                                        halign: "left"
                                        theme_text_color: "Custom"
                                        text_color: 0, 0, 0, 1 
                                        multiline: True
                                        font_size: '16sp'
                            
                            MDLabel:
                                id: second_label_f
                                padding: '1dp', '55dp', '1dp', '1dp'
                                text: 'Tasirleyijinin mohumligini 0-10 aralygyndaky bitin san bilen beyan edin.'
                                font_size: 13

                            MDTextField:
                                id: second_text_input_f
                                disabled: False
                                size_hint:.4, None
                                

                            
                            MDLabel:
                                text: 'Isi etmage bolan islegi:'

                            MDTextButton:
                                id: x_info
                                text: 'Derejelendir'
                                pos_hint: {'center_x': .3}    
                                on_release: root.open_x_menu()

                            MDLabel:
                                text: 'Isi etmage bolan islegsizlik:'

                            MDTextButton:
                                id: y_info
                                text: 'Derejelendir'
                                pos_hint: {'center_x': .3}    
                                on_release: root.open_y_menu()   

                            MDLabel:
                                text: 'Towekgel goterimini belle (risk)'

                            MDTextButton:
                                id: z_info
                                text: '0%'
                                pos_hint: {'center_x': .3}    
                                on_release: root.open_z_menu()


                            MDLabel:
                                text: 'Is ucin gerekli bolan akyl saglygy we bedeni saglyk'

                            MDBoxLayout:
                                orientation: 'horizontal'
                                spacing: dp(80)

                                MDTextButton:
                                    id: head_info
                                    text: 'Belgile'
                                    pos_hint: {'center_x': .3}
                                    on_release: root.head()

                                MDTextButton:
                                    id: body_info
                                    text: 'Belgile'
                                    pos_hint: {'center_x': .8}
                                    on_release: root.body()
                                                     

                MDTopAppBar:
                    id: calc_bottom_app_bar
                    pos_hint: {'bottom': 1}
                    elevation: 0                    
                        
                    MDIconButton:
                        icon: 'send'
                        on_release: root.control(self)
                        pos_hint: {'center_x': .96}

        MyNavigationDrawer:
            id: nav_drawer_1
            radius: 0

            MDNavigationDrawerMenu:

                MDNavigationDrawerLabel:

                DrawerClickableItem:
                    id: calc_drawer_id_0
                    icon: 'brain'
                    text: 'Psihologiya'
                    on_enter: app.enter_hover(self)
                    on_leave: app.leave_hover(self)
                    on_press: root.manager.current= 'main'    

                DrawerClickableItem:
