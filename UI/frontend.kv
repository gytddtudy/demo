                            MDLabel:
                                id: label_f
                                padding: '1dp', '55dp', '1dp', '1dp'
                                text: '4. Isin erbet manyda tasirleyyijisi barada gysgaca maglumat girizin.'
                                pos_hint: {'center_x': .01}
