                            padding: [0, 0, 0, 10]
                            on_release: app.back_main()

                MDScrollView:
                    size_hint_y: .89
                    do_scroll_x: False
                    do_scroll_y: True

                    MDList:
                        size_hint_y: None
                        height: self.minimum_height

                        OneLineListItem:                        
