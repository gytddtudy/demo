                                    TextInput:
                                        id: user_input_text_e
                                        hint_text: "..."
                                        text: ''
                                        background_color: 0, 0, 0, 0
                                        halign: "left"
                                        theme_text_color: "Custom"
                                        text_color: 0, 0, 0, 1 
                                        multiline: True
                                        font_size: '16sp'

                            MDLabel:
                                id: second_label_e
                                padding: '1dp', '55dp', '1dp', '1dp'
                                text: 'Tasirleyijinin mohumligini 0-10 aralygyndaky bitin san bilen beyan edin.'
                                font_size: 13

                            MDTextField:
                                id: second_text_input_e
                                disabled: False
