        name: 'dependent'
    
    
<Main>:
    
    MDScreen:
        id: main_md
        on_touch_down: app.on_touch_down(*args)
        on_touch_up: app.on_touch_up(*args)

        MDScreenManager:            
            MDScreen:                
    
                MDNavigationLayout:

                MDTopAppBar:
                    id: top_app_bar
                    pos_hint: {'top': 1}
                    elevation: .8

                    MDBoxLayout:
                        id: top_list
                        orientation: 'horizontal'                        

                        MDTextField:
                            id: main_search
                            icon_left: 'magnify'
                            theme_text_color: 'Custom'
                            text_color_normal: (.85, .85, .85 ,1)
                            text_color_focus: (1, 1, 1, 1)
                            line_color_focus: (.95, .95, .95, 1)
                            line_color_normal: (.85, .85, .85, 1) 
                            icon_left_color_focus: (1, 1, 1, 1)
                            icon_left_color_normal: (1, 1, 1, 1)
                            on_touch_down: app.find_word() 

                        MDIconButton:
                            icon: 'star-outline'
                            icon_size: '20sp'
                            icon_color: (1, 1 ,1, 1)
                            theme_text_color: 'Custom'
                            text_color: (1, 1, 1, 1)
                            on_release: root.get_favorites()

                        MDIconButton:
                            icon: 'globe-model'
                            icon_size: '20sp'
                            theme_text_color: 'Custom'
                            text_color: (1, 1, 1, 1) 

                MDBoxLayout:
                    orientation: 'vertical'
                    padding: [10, 65, 10, 10]
                    spacing: dp(10)

                    MDScrollView:
                        do_scroll_x: False
                        do_scroll_y: True
                        
                        MDList:
                            id: mother_lists


        MyNavigationDrawer:
            id: nav_drawer_0
            radius: 0

            MDNavigationDrawerMenu:

                MDNavigationDrawerLabel:

                DrawerClickableItem:
                    id: main_drawer_id_0
                    icon: 'brain'
                    text: 'Psihologiya'
                    on_enter: app.enter_hover(self)
                    on_leave: app.leave_hover(self)
                    on_press: root.manager.current='main'                    

                DrawerClickableItem:
                    id: main_drawer_id_1
                    icon: 'calculator'
                    text: 'Hasapla'
                    on_enter: app.enter_hover(self)
                    on_leave: app.leave_hover(self)
                    on_press: root.manager.current= 'posibility_calc_screen'

                MDNavigationDrawerDivider:
                    
                DrawerLabelItem:
                    id: main_drawer_id_2
                    icon: 'cog'
                    text: 'Sazlamalar'
                    on_enter: app.enter_hover(self)
                    on_leave: app.leave_hover(self)
                    on_press: root.manager.current= 'setting'

                DrawerLabelItem:
                    id: main_drawer_id_3
                    text: 'Maglumat'
                    icon: 'information-outline'
                    on_enter: app.enter_hover(self)
                    on_leave: app.leave_hover(self)

<Posibilyty_calc>:
    
    MDScreen:
        id: posibility_calc_md
        on_touch_down: app.on_touch_down(*args)
        on_touch_up: app.on_touch_up(*args)

        MDScreenManager:
            MDScreen:
                    
                MDNavigationLayout:
                MDTopAppBar:
                    id: calc_top_app_bar
                    pos_hint: {'top': 1}
                    elevation: 0                    
                    elevation: .8

                    MDBoxLayout:
                        orientation: 'horizontal'

                        MDTextField:
                            icon_left: 'magnify'
                            hint_text: 'Gozleg'
                            text_color_normal: (1, 1, 1 ,1)
                            text_color_focus: (1, 1, 1, 1)
                            line_color_focus: (.95, .95, .95, 1)
                            line_color_normal: (.95, .95, .95, 1) 
                            hint_text_color_normal: (.85, .85, .85, 1)
                            icon_left_color_focus: (1, 1, 1, 1)
                            icon_left_color_normal: (1, 1, 1, 1)
                            on_touch_down: app.find_word(*args)

                        MDIconButton:
                            icon: 'form-textarea'
                            icon_size: '20sp'
                            icon_color: (1, 1, 1, 1)
                            theme_text_color: 'Custom'
                            text_color: (1, 1, 1, 1)

                        MDIconButton:
                            icon: 'globe-model'
                            icon_size: '20sp'
                            theme_text_color: 'Custom'
                            text_color: (1, 1, 1, 1)
                        

                MDBoxLayout:
                    orientation: 'horizontal'
                    
                    MDIconButton:
                        padding: '10dp'
                        icon: 'restore'
                        pos_hint: {'center_x': .1, 'center_y': .85}
                        on_release: root.reset_button(self)

                MDScrollView:
                    pos_hint: {'center_x': .5, 'center_y': .32}
                    do_scroll_x: False
                    do_scroll_y: True

                    MDList:
                        height: self.minimum_height
                        size_hint_y: None
                        bar_width: 20

                                                        
                        MDGridLayout:
                            size_hint_y: None
                            height: self.minimum_height
                            cols: 0
                            rows: 25
                            spacing: '60dp'
                            padding: '20dp'

                            MDLabel:
                                id: label_c
                                padding: '1dp', '55dp', '1dp', '1dp'
                                text: '1. Ise bolan ukybynyz barada gysgaca maglumat girizin.'
                                pos_hint: {'center_x': .01}
                                font_size: 13

                        
                            MDCard:
                                id: user_input_card_c
                                size_hint: .95, None
                                radius: [15, 15, 15, 15]
                                md_bg_color: 0.90, 0.90, 0.90, 1 
                                padding: '10dp', '10dp', '10dp', '10dp'
                                pos_hint: {'center_x': .5, 'center_y': 0}

                                MDBoxLayout:
                                    orientation: 'vertical'

                                    MDIconButton:
                                        id: input_card_head
                                        icon: 'dots-horizontal'
                                        pos_hint: {'center_x': .95}
                                        on_release: pass


                                
                                    TextInput:
                                        id: user_input_text_c
                                        hint_text: "..."
                                        text: ''
                                        background_color: 0, 0, 0, 0
                                        halign: "left"
                                        theme_text_color: "Custom"
                                        text_color: 0, 0, 0, 1 
                                        multiline: True
                                        font_size: '16sp'

                                    
                
                            MDLabel:
                                id: label_d
                                padding: '1dp', '55dp', '1dp', '1dp'
                                text: '2. Is basaranyksyzlygy barada gysgaca maglumat girizin.'
                                pos_hint: {'center_x': .01}
                                font_size: 13

                        
                            MDCard:
                                id: user_input_card_d
                                size_hint: .95, None
                                radius: [15, 15, 15, 15]
                                md_bg_color: 0.90, 0.90, 0.90, 1 
                                padding: '10dp', '10dp', '10dp', '10dp'
                                pos_hint: {'center_x': .5, 'center_y': 0}

                                MDBoxLayout:
                                    orientation: 'vertical'

                                    MDIconButton:
                                        id: input_card_head
                                        icon: 'dots-horizontal'
                                        pos_hint: {'center_x': .95}
                                        on_release: pass


                                
                                    TextInput:
                                        id: user_input_text_d
                                        hint_text: "..."
                                        text: ''
                                        background_color: 0, 0, 0, 0
                                        halign: "left"
                                        theme_text_color: "Custom"
                                        text_color: 0, 0, 0, 1 
                                        multiline: True
                                        font_size: '16sp'
                            
                            MDLabel:
                                id: label_e
                                padding: '1dp', '55dp', '1dp', '1dp'
                                text: '3. Isin gowy manyda tasirleyyijisi barada gysgaca maglumat girizin.'
                                pos_hint: {'center_x': .01}
                                font_size: 13

                        
                            MDCard:
                                id: user_input_card_e
                                size_hint: .95, None
                                radius: [15, 15, 15, 15]
                                md_bg_color: 0.90, 0.90, 0.90, 1 
                                padding: '10dp', '10dp', '10dp', '10dp'
                                pos_hint: {'center_x': .5, 'center_y': 0}

                                MDBoxLayout:
                                    orientation: 'vertical'

                                    MDIconButton:
                                        id: input_card_head
                                        icon: 'dots-horizontal'
