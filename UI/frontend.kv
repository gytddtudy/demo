#:import NoTransition kivy.uix.screenmanager.NoTransition

<DrawerClickableItem@MDNavigationDrawerItem>

<DrawerLabelItem@MDNavigationDrawerItem>
    
<MyNavigationDrawer@MDNavigationDrawer>


ScreenManager:
    transition: NoTransition()
    Main:
        name: 'main'
    Posibilyty_calc:
        name: 'posibility_calc_screen'
    Setting:
        name: 'setting'

    Characters:
        name: 'characters'
    Child:
        name: 'child'
    Acne: 
        name: 'acne'
    Women:
        name: 'women'
    Man:
        name: 'man'
    Learn:
        name: 'learn'
    Battle:
        name: 'battle'

    Shy:
        name: 'shy'
    Otism: 
        name: 'otism'
    Asperger:
        name: 'asperger'
    Schizoid:
        name: 'schizoid'
    Agresive:
        name: 'agresive'
    Paranoid: 
        name: 'paranoid'
    Histrionic:
        name: 'histrionic'
    Narcissism:
        name: 'narcissism'
    Mazochism: 
        name: 'mazochism'
    Depresive: 
        name: 'depresive'
